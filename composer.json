{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": "5.3.*",
        "hisorange/browser-detect": "2.*",
        "tymon/jwt-auth": "0.5.*",
        "league/fractal": "^0.13.0",
        "dingo/api": "1.0.*@dev",
        "webpatser/laravel-uuid": "2.*",
        "geoip2/geoip2": "~2.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.0",
        "symfony/css-selector": "3.1.*",
        "symfony/dom-crawler": "3.1.*"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "files": [
            "app/Support/helpers.php", 
            "app/Modules/Area/Exceptions.php", 
            "app/Modules/Collection/Exceptions.php", 
            "app/Modules/File/Exceptions.php", 
            "app/Modules/Geo/Exceptions.php", 
            "app/Modules/Territory/Exceptions.php", 
            "app/Modules/User/Exceptions.php"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
